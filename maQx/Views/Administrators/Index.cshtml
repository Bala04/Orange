@{
    Layout = "~/Views/Shared/_TablePage.cshtml";
}

@section Scripts{
    <script>
        angular.module("tabularApp").service("DataService", ['$resource', function ($resource) {
            return {
                query: function () {
                    return $resource('get/administrators').get();
                },
                init: function () {
                    return [{ a: 'Code', b: 'Code' }, { a: 'Name', b: 'Name' }, { a: 'Email', b: 'Email' }, { a: 'Organization', b: 'Organization' }];
                }
            }
        }]);
    </script>
}