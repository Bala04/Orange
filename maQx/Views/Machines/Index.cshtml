@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_TablePage.cshtml";
}


@section Scripts{
    <script>
        angular.module("tabularApp").service("DataService", ['$resource', function ($resource) {
            return {
                query: function () {
                    return $resource('get/Machines').get();
                },
                init: function () {
                    return [{ a: 'Code', b: 'Code' }, { a: 'Description', b: 'Description' }, { a: 'MachineTypeString', b: 'Machine Type' }, { a: 'MinLoad', b: 'Min Load' }, { a: 'MaxLoad', b: 'Max Load' }, { a: 'Modified', b: 'Modified' }];
                }
            }
        }]);
    </script>
}



