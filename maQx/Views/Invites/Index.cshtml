@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_TablePage.cshtml";
}

@section Scripts{
    <script>
        angular.module("tabularApp").service("DataService", ['$resource', function ($resource) {
            return {
                query: function () {
                    return $resource('get/invites').get();
                },
                init: function () {
                    return [{ a: 'Username', b: 'Username' }, { a: 'Email', b: 'Email' }, { a: 'Modified', b: 'Modified' }];
                }
            }
        }]);
    </script>
}
